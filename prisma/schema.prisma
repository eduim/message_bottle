generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id                   Int      @id 
  githubToken         String 
  githubTokenExpires Int
  githubUser          String
  token                String?
  intStartDate       Boolean  @default(false)
  startDate           Int?
}

